{
  "name": "shared-chat",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "bin/debug/modules/chat/libs/shared-chat",
        "main": "src/modules/eleoncorechat/libs/shared-chat/src/index.ts",
        "tsConfig": "src/modules/eleoncorechat/libs/shared-chat/tsconfig.json",
        "webpackConfig": "src/modules/eleoncorechat/libs/shared-chat/webpack.config.ts"
      },
      "configurations": {
        "production": {},
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
