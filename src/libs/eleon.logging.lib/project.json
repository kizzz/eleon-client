{
  "name": "eleon.logging.lib",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "bin/debug/libs/eleon.logging.lib",
        "main": "src/libs/eleon.logging.lib/src/index.ts",
        "tsConfig": "src/libs/eleon.logging.lib/tsconfig.json",
        "webpackConfig": "src/libs/eleon.logging.lib/webpack.config.ts"
      },
      "configurations": {
        "production": {},
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
